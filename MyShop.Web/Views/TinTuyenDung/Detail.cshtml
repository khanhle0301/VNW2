                            <div class="pull-right">

                                <button type="button" class="btn btn-defautl btn-outline" tabindex="11" data-dismiss="modal">Trở Về</button>

                                <button type="button" id="applySendProcessBtn" class="btn btn-primary disabled" tabindex="11">Nộp Đơn</button>

                                <button type="submit" id="applySendingProcessBtn" style="display: none;" class="btn btn-default" tabindex="10" disabled="">
                                    <img src="/assets/client/images.vietnamworks.com/loading.gif" alt=""> Đang Nộp Đơn
                                </button>
                            </div>
                        </div>
                    </div>
                </div>
            </form>
        </div>
