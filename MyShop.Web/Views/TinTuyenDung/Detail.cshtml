@model MyShop.Web.Models.DetailVm

@{
    ViewBag.Title = "Detail";
    Layout = "~/Views/Shared/_Layout.cshtml";

    TimeSpan interval = DateTime.Now.Subtract(Model.TinTuyenDung.UpdatedDate.Value);

    var days = interval.Days;
}
<div class="main-wrapper ">
    <section class="main-content container" id="content">
        <!-- Apply Form-->
        <div class="modal fade apply-form-modal" id="myModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
            <form class="form-horizontal" id="applyJobForm" enctype="multipart/form-data" method="post" action="https://www.vietnamworks.com/jobseekers/apply_on_oneclick.php">
                <input type="hidden" name="jobId" id="jobId" value="811454">

                <div class="modal-dialog">
                    <div class="modal-content">
                        <div class="modal-header">
                            <button type="button" class="close" data-dismiss="modal">
                                <span aria-hidden="true" tabindex="8">×</span>
                                <span class="sr-only">Close</span>
                            </button>

                            <h4 class="modal-title" id="myModalLabel">
                                Bạn đang ứng tuyển cho vị trí
                                <strong>5 Sr. Fullstack Developers ( Javascript, PHP, Python )</strong><br>
                                <em class="small" id="appPreferLanguage" style="display: none;">Nhà tuyển dụng ưu tiên hồ sơ ứng tuyển viết bằng tiếng <strong id="appPreferLanguageName"></strong>.</em>
                            </h4>
                        </div>

                        <div class="modal-body">
                            <div class="resume-header">
                                <div class="row">
                                    <!-- Resume Avatar -->
                                    <div class="col-sm-3 col-xs-4 user-pic">
                                        <div class="avatar img-circle text-center">
                                            <div class="avatar-sample">
                                                <i class="fa fa-3x fa-user"></i>
                                            </div>
                                        </div>
                                    </div>

                                    <div class="col-sm-9 col-xs-8">
                                        <!-- Full Name -->
                                        <h4 class="m-t-md">
                                            <span id="app-fullName"></span>
                                        </h4>

                                        <!-- Job Title -->
                                        <div class="m-t-xs app-title">
                                            <a href="javascript:void(0);" class="editable" data-placeholder="Nhập chức danh của bạn" tabindex="1" id="app-title" data-optional="1"></a>
                                        </div>

                                        <input type="hidden" name="appJobTitle" id="appJobTitle" value="">
                                    </div>
                                </div>
                            </div>

                            <div id="resume" class="resume-body">
                                <!-- Email Address -->
                                <div class="form-group m-t-sm">
                                    <label for="app-resume" class="col-sm-3 col-xs-12 control-label">Email</label>
                                    <div class="col-sm-9 col-xs-12">
                                        <div class="form-control-static" id="app-email"></div>

                                        <div class="error-message">Để nộp đơn vui lòng kích hoạt tài khoản thông qua email của bạn.</div>
                                    </div>
                                </div>

                                <!-- Phone Number -->
                                <div class="form-group m-t-sm">
                                    <label for="app-resume" class="col-sm-3 col-xs-12 control-label">Số điện thoại</label>
                                    <div class="col-sm-9 col-xs-12">
                                        <div class="form-control-static phone-number">
                                            <a id="app-phone" href="#" data-type="text" data-placeholder="Nhập số điện thoại liên lạc" data-pk="1" class="editable" value="Enter phone number" tabindex="4"></a>

                                            <br>

                                            <span class="error-message" style="display: none;" id="errorAppPhone">Vui lòng nhập số điện thoại.</span>

                                            <input type="hidden" name="appPhoneNumber" id="appPhoneNumber" value="">
                                        </div>
                                    </div>
                                </div>

                                <!-- Select Resume Block -->
                                <div class="form-group awe-check select-resume">
                                    <label for="app-resume" class="col-sm-3 col-xs-12 control-label">
                                        Resume
                                    </label>

                                    <div class="col-sm-9 col-xs-12">
                                        <div id="applyResume" class="m-t-xs"></div>

                                        <input type="hidden" name="onlineResumeId" id="appDefaultResume" value="">
                                        <input type="hidden" name="attachmentResume" id="attachmentResume" value="">

                                        <!-- Upload New CV -->
                                        <div class="row select-resume__item select-resume__upload-cv m-t-xs">
                                            <div class="col-sm-12">
                                                <div class="radio radio-success">
                                                    <input tabindex="7" type="radio" name="resumeApply" id="optionsRadios3" value="newAttachment" data-type="new_attachment">

                                                    <label for="fileAttach">
                                                        <a class="relative">
                                                            Chọn hồ sơ mới
                                                            <i class="glyphicon glyphicon-upload" data-toggle="tooltip" title="Replace" data-placement="top"></i>
                                                        </a>
                                                    </label>

                                                    <input type="file" name="resumeFile" id="fileAttach" value="Attach new" tabindex="8" class="transparent absolute">

                                                    <span id="resume-filename" class="small m-l-md gray text-clip-50"></span>
                                                </div>

                                                <span class="small gray-light pull-left m-l-lg m-r-lg">Hỗ trợ định dạng .doc, .docx, .pdf, nhỏ hơn 2048KB.</span>
                                            </div>
                                        </div>

                                        <br>

                                        <span class="error-message" style="display: none;" id="errorAppResume">Vui lòng chọn hồ sơ đính kèm</span>
                                        <span class="error-message" style="display: none;" id="errorAppResumeFile">Vui lòng chọn tập tin đính kèm với định dạng .doc, .docx, .pdf, nhỏ hơn 2048KB</span>
